{
    "name": "sneha_w/static-content-manager",
    "description": "StaticContentManager plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4",
        "cakephp/cakephp": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "StaticContentManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StaticContentManager\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
